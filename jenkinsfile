pipeline {
    agent any
    stages {
        stage('Clone sources') {
            steps {
                git url: 'https://github.com/imkunalll/Manager.git'
            }
        }
        stage('SonarQube analysis') {
            steps {
                withSonarQubeEnv('SonarQube') {
                   bat " sonar:sonar -Dsonar.projectKey=Iknons8_ -Dsonar.host.url=http://localhost:9000 -Dsonar.login=3de26341ef104b2375c4d93722997204a93308ab"
                }
            }
        }
        stage("Quality gate") {
            steps {
                waitForQualityGate abortPipeline: true
            }
        }
    }
    }
