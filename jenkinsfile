pipeline {
agent any
    stages {
        stage('Clone sources') {
            steps {
                git url: 'https://github.com/imkunalll/Manager.git'
            }
        }

        stage('SonarQube analysis') {
            steps {
                withSonarQubeEnv('SonarQube') {
                    bat "./gradlew sonarqube"
                }
            }
        }
        stage("Quality gate") {
            steps {
                waitForQualityGate abortPipeline: true
            }
        }
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'building'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
      
        stage('Release') {
            steps {
                echo 'Realeasing'
            }
        }
  }
  }
